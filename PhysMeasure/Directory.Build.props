    <NeutralLanguage>en-US</NeutralLanguage>

    <Authors>Kilo Bravo</Authors>
    <WarningsAsErrors>True</WarningsAsErrors>

    <PackageTags>physical;measure;quantity;unit;SI;system;conversion;physics;engineering</PackageTags>
    <PackageProjectUrl>https://github.com/KiloBravoLima/PhysicalMeasure</PackageProjectUrl>
    <PackageLicenseUrl>https://opensource.org/licenses/MS-PL</PackageLicenseUrl>

